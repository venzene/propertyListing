<%- include('../partials/header', { isAuthenticated: isAuthenticated }) %>

<div class="container mt-5">
  <h1 class="mb-4 text-center">Browse Properties</h1>

  <div class="row">
    <% favoriteProperties.forEach(function(property) { %>
      <% if (property) { %>
        <div class="col-md-4 mb-4">
          <%- include('./options', { property: property }) %>
        </div>
      <% } %>
    <% }) %>
  </div>
</div>

<%- include('../partials/footer') %>
